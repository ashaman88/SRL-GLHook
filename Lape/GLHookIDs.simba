Function InArray(Arr: GLE_Equipment_Array; Which: GLE_Equipment): Boolean;
var
  I: Integer;
Begin
  For I := 0 To High(Arr) Do
    If (Arr[I] = Which) Then
    Begin
      Result := True;
      Exit;
    End;
End;
